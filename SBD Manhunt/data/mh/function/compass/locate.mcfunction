# Set dimension

execute as @a[nbt={SelectedItem:{id:"minecraft:compass", count:1}}] if entity @a[team=Runner,distance=0..] if data entity @s {Dimension: "minecraft:overworld"} if entity @a[team=Runner,gamemode=!spectator,distance=0..] run data modify storage mh:storage Item set value {id:"minecraft:compass", count:1, components:{"minecraft:custom_name":'{"color":"aqua", "italic":false, "text":"Tracker Compass"}', "minecraft:lodestone_tracker": {tracked: 0b, target: {pos:[I;0,0,0], dimension:"minecraft:overworld"}}  }}
execute as @a[nbt={SelectedItem:{id:"minecraft:compass", count:1}}] if entity @a[team=Runner,distance=0..] if data entity @s {Dimension: "minecraft:the_nether"} if entity @a[team=Runner,gamemode=!spectator,distance=0..] run data modify storage mh:storage Item set value {id:"minecraft:compass", count:1, components:{"minecraft:custom_name":'{"color":"aqua", "italic":false, "text":"Tracker Compass"}', "minecraft:lodestone_tracker": {tracked: 0b, target: {pos:[I;0,0,0], dimension:"minecraft:the_nether"}}  }}
execute as @a[nbt={SelectedItem:{id:"minecraft:compass", count:1}}] if entity @a[team=Runner,distance=0..] if data entity @s {Dimension: "minecraft:the_end"} if entity @a[team=Runner,gamemode=!spectator,distance=0..] run data modify storage mh:storage Item set value {id:"minecraft:compass", count:1, components:{"minecraft:custom_name":'{"color":"aqua", "italic":false, "text":"Tracker Compass"}', "minecraft:lodestone_tracker": {tracked: 0b, target: {pos:[I;0,0,0], dimension:"minecraft:the_end"}}  }}

